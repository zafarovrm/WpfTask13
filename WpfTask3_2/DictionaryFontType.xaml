<ResourceDictionary xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
                    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml" 
                    xmlns:fonttype="clr-namespace:System.Collections;assembly=mscorlib" 
                    xmlns:sys="clr-namespace:System;assembly=mscorlib">
    <fonttype:ArrayList x:Key="font">
        <sys:String>Arial</sys:String>
        <sys:String>Calibri</sys:String>
        <sys:String>ISOCPEUR</sys:String>        
        <sys:String>Times New Roman</sys:String>
        <sys:String>Verdana</sys:String>
    </fonttype:ArrayList>
</ResourceDictionary>